<ul class="nav nav-tabs">
    <li role="presentation">
        <a data-test="gitReadMe" href="{{config.gitURL}}/{{repoName}}/readme">Read Me</a>
    </li>
    <li role="presentation">
        <a data-test="gitFiles" href="{{config.gitURL}}/{{repoName}}/files">Files</a>
    </li>
    <li role="presentation" class="active">
        <a data-test="gitBranches" href="{{config.gitURL}}/{{repoName}}/branches">Branch</a>
    </li>
    <li role="presentation">
        <a data-test="gitHistory" href="{{config.gitURL}}/{{repoName}}/history">History</a>
    </li>
    {{#if setting}}
    <li role="presentation">
        <a data-test="gitSetting" href="{{config.gitURL}}/{{repoName}}/setting">Setting</a>
    </li>
    {{/if}}
</ul>
<div class="col-md-12 tab-border branch-structure">
    {{#if verify}}
    <div class="row">
        <div class="col-md-12">
            Head Branch:
            <table class="table">
                <thead>
                    <tr>
                        <td><strong>{{branchName}}</strong></td>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            Active Branches:
            <table class="table table-border">
                <thead>
                    {{#each branches}}
                    <tr>
                        <td>{{this}}</td>
                    </tr>
                    {{/each}}
                </thead>
            </table>
        </div>
    </div>
    {{else}}
    <div class="row">
        <div class="col-md-12 text-center">
            This repository is empty
        </div>
    </div>
    {{/if}}
</div>