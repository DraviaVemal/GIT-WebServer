<ul class="nav nav-tabs">
    <li role="presentation">
        <a data-test="gitReadMe" href="{{config.gitURL}}/{{repoName}}/readme">Read Me</a>
    </li>
    <li role="presentation">
        <a data-test="gitFiles" href="{{config.gitURL}}/{{repoName}}/files">Files</a>
    </li>
    <li role="presentation">
        <a data-test="gitBranches" href="{{config.gitURL}}/{{repoName}}/branches">Branch</a>
    </li>
    <li role="presentation">
        <a data-test="gitPullRequest" href="{{config.gitURL}}/{{repoName}}/pullrequest">Pull Request</a>
    </li>
    <li role="presentation" class="active">
        <a data-test="gitHistory" href="{{config.gitURL}}/{{repoName}}/history">History</a>
    </li>
    {{#if setting}}
    <li role="presentation">
        <a data-test="gitSetting" href="{{config.gitURL}}/{{repoName}}/setting">Setting</a>
    </li>
    {{/if}}
</ul>
<div class="col-md-12 tab-border history-structure">
    <table class="table tab-border">
        <tr>
            <th>Author</th>
            <th>Message</th>
            <th>TimeStamp</th>
            <th>SHA-1</th>
            <th>Ref. branch</th>
        </tr>
        {{#each history}}
        <tr>
            <td>{{author}}</td>
            <td>{{message}}</td>
            <td>{{timestamp}}</td>
            <td>{{sha}}</td>
            <td>{{branch}}</td>
        </tr>
        {{/each}}
    </table>
</div>